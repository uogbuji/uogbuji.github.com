    <updated>2012-10-27</updated>
    <link href="http://theofipress.webs.com/ogbujiuche.htm"/>
    <category term="@choice"/>
    <source>
      <id>http://theofipress.webs.com/</id>
      <title>The Ofi Press Magazine</title>
